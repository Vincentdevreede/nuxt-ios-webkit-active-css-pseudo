{
  "name": "nuxt-ios-webkit-active-css-pseudo",
  "version": "1.0.0",
  "description": "A simple, lightweight Nuxt plugin that resolves the :active CSS pseudo-class issue in iOS WebKit browsers.",
  "repository": "https://github.com/Vincentdevreede/nuxt-ios-webkit-active-css-pseudo",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build --stub false",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint ."
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.3.2",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.2",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.23.0",
    "nuxt": "^3.16.2",
    "typescript": "~5.8.2",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808"
}
